<!DOCTYPE html>
<html>
  <head>
    <%- include ("../partials/header.ejs") %>
  </head>

  <body>
    <%- include ("../partials/nav.ejs") %>

    <div class="container">
      <h2>Database Results</h2>

      <ul>

        <% for (var i =0; i < rows.length; i++) { %>
          <tr>
            <td><%= rows[i].name %></td>
            <td><%= rows[i].weight %></td>
            <td><%= rows[i].height %></td>
            <td><%= rows[i].haircolor %></td>
            <td><%= rows[i].gpa %></td>
            <td>
            <form class="" action="/changeuser" method="get">
              
              <button id="button" type="submit" name="adduserbutton">Change User</button>
              <td><%= rows[i].id %></td>
            </form>
            </td>
            <br>
          </tr>
        <% } %>

      </ul>
    </div>
  </body>
</html>
